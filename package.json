{
  "name": "speedwell",
  "version": "1.0",
  "private": true,
  "description": "A Next.js template for small businesses featuring high-quality feminine design. Build and deploy a professional small business website with speed and get highly ranked on Google. Manage content and images with AI using Gallop software. No CMS required.",
  "homepage": "https://speedwell.gallop.software",
  "author": {
    "name": "Gallop",
    "url": "https://gallop.software"
  },
  "contributors": [
    {
      "name": "Chris Baldelomar",
      "url": "https://github.com/webplantmedia"
    },
    {
      "name": "Niel Wostan",
      "url": "https://github.com/NielWostan"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gallop-software/speedwell"
  },
  "bugs": {
    "url": "https://github.com/gallop-software/speedwell/issues"
  },
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "npm run blog && next build",
    "start": "next start",
    "lint": "eslint src/",
    "lint:file": "eslint",
    "ts": "tsc --noEmit",
    "lint:next": "eslint src/ --rule 'gallop/no-client-blocks: off' --rule 'gallop/no-container-in-section: off' --rule 'gallop/prefer-component-props: off'",
    "lint:gallop": "eslint src/blocks/ --rule 'gallop/no-client-blocks: warn' --rule 'gallop/no-container-in-section: warn' --rule 'gallop/prefer-component-props: warn'",
    "audit": "gallop audit",
    "audit:strict": "gallop audit --strict",
    "audit:json": "gallop audit --json",
    "generate:ai-rules": "gallop generate .cursorrules && gallop generate .github/copilot-instructions.md",
    "images": "node _scripts/process-images.js",
    "images:reset": "rm -rf public/images && npm run images",
    "favicon": "node _scripts/generate-favicon.js",
    "featured-image": "node _scripts/generate-featured-image.js",
    "search": "./_scripts/generate-search.sh",
    "blog": "node _scripts/generate-blog-metadata.mjs",
    "blocks": "node _scripts/generate-blocks-catalog.mjs",
    "blocks:screenshots": "node _scripts/generate-blocks-catalog.mjs --screenshots",
    "blocks:sort": "node _scripts/generate-blocks-catalog.mjs --sort",
    "blocks:lite": "node _scripts/convert-pro-blocks.mjs",
    "layouts": "node _scripts/generate-layouts-catalog.mjs",
    "layouts:screenshots": "node _scripts/generate-layouts-catalog.mjs --screenshots",
    "layouts:sort": "node _scripts/generate-layouts-catalog.mjs --sort",
    "env": "./push-env-to-vercel.sh",
    "env:prod": "./push-env-to-vercel.sh production",
    "env:prev": "./push-env-to-vercel.sh preview",
    "fonts": "node _scripts/compress-fonts.js",
    "fonts:use": "node src/styles/fonts/useFonts.js",
    "refresh": "rm -rf node_modules .next",
    "refresh:install": "npm run refresh && npm install",
    "clean": "rm -rf node_modules .next package-lock.json",
    "clean:install": "npm run clean && npm install",
    "update:check": "npx npm-check-updates",
    "update:patch": "npx npm-check-updates -u --target patch && npm run clean:install",
    "update:minor": "npx npm-check-updates -u --target minor && npm run clean:install",
    "update:major": "npx npm-check-updates -u --target latest && npm run clean:install",
    "update:interactive": "npx npm-check-updates -i",
    "update:doctor": "npx npm-check-updates --doctor -u",
    "update:canon": "npm update @gallop.software/canon"
  },
  "dependencies": {
    "@algolia/autocomplete-core": "^1.19.4",
    "@headlessui/react": "^2.2.9",
    "@iconify/icons-heroicons": "^1.2.9",
    "@iconify/icons-lucide": "^1.2.135",
    "@iconify/icons-mdi": "^1.2.48",
    "@iconify/icons-simple-icons": "^1.2.74",
    "@next/third-parties": "^16.1.2",
    "@sindresorhus/slugify": "^3.0.0",
    "@vercel/analytics": "^1.6.1",
    "@vimeo/player": "^2.30.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "flexsearch": "^0.8.212",
    "framer-motion": "^12.26.2",
    "luxon": "^3.7.2",
    "next": "^16.1.2",
    "react": "^19",
    "react-dom": "^19.2.3",
    "react-highlight-words": "^0.21.0",
    "react-intersection-observer": "^10.0.0",
    "swiper": "^12.0.3",
    "valtio": "^2.3.0",
    "yet-another-react-lightbox": "^3.28.0"
  },
  "devDependencies": {
    "@gallop.software/canon": "^2.4.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-highlight-words": "^0.20.1",
    "@types/vimeo__player": "^2.18.3",
    "eslint": "^9",
    "eslint-config-next": "^16.1.2",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.0",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "puppeteer": "^24.35.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "ttf2woff2": "^8.0.0",
    "typescript": "^5",
    "xml2js": "^0.6.2"
  }
}
