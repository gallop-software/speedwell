{
  "name": "speedwell",
  "version": "1.1.24",
  "private": true,
  "description": "A Next.js template for small businesses featuring high-quality feminine design. Build and deploy a professional small business website with speed and get highly ranked on Google. Manage content and images with AI using Gallop software. No CMS required.",
  "homepage": "https://speedwell.gallop.software",
  "author": {
    "name": "Gallop",
    "url": "https://gallop.software"
  },
  "contributors": [
    {
      "name": "Chris Baldelomar",
      "url": "https://github.com/webplantmedia"
    },
    {
      "name": "Niel Wostan",
      "url": "https://github.com/NielWostan"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gallop-software/speedwell"
  },
  "bugs": {
    "url": "https://github.com/gallop-software/speedwell/issues"
  },
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "studio": "studio --workspace .",
    "build": "npm run blog && next build",
    "start": "next start",
    "lint": "eslint src/",
    "lint:file": "eslint",
    "prettier": "prettier --write .",
    "ts": "tsc --noEmit",
    "lint:next": "eslint src/ --rule 'gallop/no-client-blocks: off' --rule 'gallop/no-container-in-section: off' --rule 'gallop/prefer-component-props: off'",
    "lint:gallop": "eslint src/blocks/ --rule 'gallop/no-client-blocks: warn' --rule 'gallop/no-container-in-section: warn' --rule 'gallop/prefer-component-props: warn'",
    "audit": "gallop audit",
    "audit:strict": "gallop audit --strict",
    "audit:json": "gallop audit --json",
    "generate:ai-rules": "gallop generate .cursorrules && gallop generate .github/copilot-instructions.md",
    "search": "./_scripts/generate-search.sh",
    "blog": "node _scripts/generate-blog-metadata.mjs",
    "env": "./_scripts/push-env-to-vercel.sh",
    "env:prod": "./_scripts/push-env-to-vercel.sh production",
    "env:prev": "./_scripts/push-env-to-vercel.sh preview",
    "refresh": "rm -rf node_modules .next && npm install",
    "clean": "rm -rf node_modules .next package-lock.json && npm install",
    "update:check": "npx npm-check-updates",
    "update:patch": "npx npm-check-updates -u --target patch && npm run clean",
    "update:minor": "npx npm-check-updates -u --target minor && npm run clean",
    "update:major": "npx npm-check-updates -u --target latest && npm run clean",
    "update:interactive": "npx npm-check-updates -i",
    "update:doctor": "npx npm-check-updates --doctor -u",
    "update:canon": "npm update @gallop.software/canon",
    "unused": "knip",
    "check": "npm run lint && npm run ts"
  },
  "dependencies": {
    "@algolia/autocomplete-core": "^1.19.4",
    "@headlessui/react": "^2.2.9",
    "@iconify/icons-heroicons": "^1.2.9",
    "@iconify/icons-lucide": "^1.2.135",
    "@iconify/icons-mdi": "^1.2.48",
    "@iconify/icons-simple-icons": "^1.2.74",
    "@next/third-parties": "^16.1.6",
    "@sindresorhus/slugify": "^3.0.0",
    "@vercel/analytics": "^1.6.1",
    "@vimeo/player": "^2.30.2",
    "clsx": "^2.1.1",
    "flexsearch": "^0.8.212",
    "framer-motion": "^12.30.0",
    "jsdom": "^27.4.0",
    "luxon": "^3.7.2",
    "next": "^16.1.6",
    "react": "^19",
    "react-dom": "^19.2.4",
    "react-highlight-words": "^0.21.0",
    "react-intersection-observer": "^10.0.2",
    "swiper": "^12.1.0",
    "valtio": "^2.3.0",
    "yet-another-react-lightbox": "^3.28.0"
  },
  "devDependencies": {
    "@gallop.software/canon": "^2.26.0",
    "@gallop.software/studio": "^2.3.172",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-highlight-words": "^0.20.1",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "^16.1.6",
    "knip": "^5.83.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "puppeteer": "^24.36.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5"
  }
}
